{
  "name": "runner",
  "version": "0.0.1",
  "license": "proprietary",
  "private": true,
  "scripts": {
    "build": "parcel build src/browser.ts --target browser && parcel build src/server.ts --target server"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "cypress": "^4.6.0",
    "cypress-wait-until": "^1.7.1",
    "express": "^4.17.1",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "parcel": "next"
  },
  "targets": {
    "server": {
      "engines": {
        "node": "^12.0.0"
      },
      "outputFormat": "commonjs",
      "includeNodeModules": true,
      "isLibrary": false
    },
    "browser": {
      "includeNodeModules": true,
      "isLibrary": false,
      "outputFormat": "global"
    }
  }
}
